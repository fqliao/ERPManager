            <TextField fx:id="productNumField" prefHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="productPriceField" prefHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
