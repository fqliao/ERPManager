      <Button fx:id="editRankButton" defaultButton="true" layoutX="113.0" layoutY="258.0" mnemonicParsing="false" onAction="#handleEditRank" prefHeight="24.0" prefWidth="102.0" text="修改" />
      <Button fx:id="deleteRankButton" layoutX="236.0" layoutY="258.0" mnemonicParsing="false" onAction="#handleDeleteRank" prefHeight="24.0" prefWidth="102.0" text="删除" />
   </children>
</AnchorPane>
